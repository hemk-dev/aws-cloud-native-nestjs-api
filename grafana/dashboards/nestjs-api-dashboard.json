{
  "id": null,
  "uid": "nestjs-api",
  "title": "NestJS API Monitoring",
  "tags": ["nestjs", "api", "application"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 0,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "rate(http_requests_total[5m])",
          "legendFormat": "{{method}} {{route}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "id": 2,
      "title": "HTTP Request Duration (p95 / p50)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "title": "Process CPU Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "rate(process_cpu_user_seconds_total{app=\"nestjs-api\"}[5m]) * 100",
          "legendFormat": "User CPU %",
          "refId": "A"
        },
        {
          "expr": "rate(process_cpu_system_seconds_total{app=\"nestjs-api\"}[5m]) * 100",
          "legendFormat": "System CPU %",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "id": 4,
      "title": "Active Handles",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 12, "y": 8},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "nodejs_active_handles_total{app=\"nestjs-api\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 100, "color": "yellow"},
              {"value": 500, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Active Requests",
      "type": "stat",
      "gridPos": {"h": 4, "w": 6, "x": 18, "y": 8},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "nodejs_active_requests_total{app=\"nestjs-api\"}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"value": null, "color": "green"},
              {"value": 50, "color": "yellow"},
              {"value": 200, "color": "red"}
            ]
          }
        }
      }
    },
    {
      "id": 5,
      "title": "Heap Memory Usage",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "nodejs_heap_size_total_bytes{app=\"nestjs-api\"}",
          "legendFormat": "Heap Total",
          "refId": "A"
        },
        {
          "expr": "nodejs_heap_size_used_bytes{app=\"nestjs-api\"}",
          "legendFormat": "Heap Used",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "title": "Event Loop Lag",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "nodejs_eventloop_lag_seconds{app=\"nestjs-api\"}",
          "legendFormat": "Event Loop Lag",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "id": 7,
      "title": "Resident Memory (RSS)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "process_resident_memory_bytes{app=\"nestjs-api\"}",
          "legendFormat": "RSS Memory",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    },
    {
      "id": 8,
      "title": "Open File Descriptors",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
      "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
      "targets": [
        {
          "expr": "process_open_fds{app=\"nestjs-api\"}",
          "legendFormat": "Open FDs",
          "refId": "A"
        },
        {
          "expr": "process_max_fds{app=\"nestjs-api\"}",
          "legendFormat": "Max FDs",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          }
        },
        "overrides": []
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {}
}
